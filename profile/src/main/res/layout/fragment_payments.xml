<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/payment_user_Cl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/rounded_dialog">

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/earning_profile_app_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/cpTransparent"
            app:elevation="@dimen/dimen_0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/payment_user_appbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <ImageView
                    android:id="@+id/back_navigation_button_earning_profile"
                    android:layout_width="@dimen/dimen_50"
                    android:layout_height="@dimen/dimen_60"
                    android:padding="@dimen/dimen_15"
                    android:layout_marginStart="@dimen/dimen_5"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ic_back_arrow" />

                <TextView
                    android:id="@+id/user_name_sitting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:letterSpacing="0.02"
                    android:text="@string/payment_p"
                    android:textColor="@color/cpTextDark"
                    android:textSize="@dimen/font_size_17"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@id/back_navigation_button_earning_profile"
                    app:layout_constraintStart_toEndOf="@+id/back_navigation_button_earning_profile"
                    app:layout_constraintTop_toTopOf="@id/back_navigation_button_earning_profile" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/save_button"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen_40"
                    android:layout_marginRight="@dimen/dimen_15"
                    android:background="@drawable/round_viewbutton"
                    android:elevation="@dimen/dimen_0"
                    android:gravity="center"
                    android:paddingVertical="@dimen/dimen_5"
                    android:paddingHorizontal="@dimen/dimen_15"
                    android:stateListAnimator="@null"
                    android:text="@string/done"
                    android:textAllCaps="false"
                    android:textColor="@color/cpHelpText"
                    android:textSize="@dimen/font_size_16"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ProgressBar
                    android:id="@+id/save_progressBar"
                    style="?android:attr/progressBarStyle"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:layout_gravity="center"
                    android:background="@drawable/circle_shape_blue"
                    android:indeterminateTintMode="src_atop"
                    android:indeterminateTint="@color/cpPrimeBlue"
                    android:translationZ="@dimen/dimen_10"
                    android:visibility="gone"
                    app:layout_constraintStart_toStartOf="@id/save_button"
                    app:layout_constraintEnd_toEndOf="@id/save_button"
                    app:layout_constraintTop_toTopOf="@id/save_button"
                    app:layout_constraintBottom_toBottomOf="@id/save_button"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.appbar.AppBarLayout>

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_40"
            android:text="@string/myworld_payment"
            android:textColor="@color/cpTextDark"
            android:textSize="@dimen/font_size_24"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/earning_profile_app_bar" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_15"
            android:background="@drawable/myspace_round_button"
            android:padding="@dimen/dimen_5"
            app:layout_constraintBottom_toBottomOf="@+id/user_upiName"
            app:layout_constraintTop_toTopOf="@+id/user_upiId"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="@dimen/dimen_20"
                android:layout_height="@dimen/dimen_20"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_google_icon" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_3"
                android:text="Pay"
                android:textColor="@color/cpTextDark"
                android:textSize="@dimen/font_size_14"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/imageView17"
                app:layout_constraintStart_toEndOf="@+id/imageView17"
                app:layout_constraintTop_toTopOf="@+id/imageView17"
                app:layout_constraintVertical_bias="0.5" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <EditText
            android:id="@+id/user_upiId"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00000000"
            android:gravity="center_vertical"
            android:hint="UPI ID"
            android:inputType="text"
            android:layout_marginTop="@dimen/dimen_10"
            android:paddingLeft="@dimen/dimen_10"
            android:textColor="@color/cpTextDark"
            android:textSize="@dimen/font_size_18"
            app:layout_constraintTop_toBottomOf="@+id/textView37"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/user_upiName"
            app:layout_constraintStart_toEndOf="@+id/constraintLayout2" />

        <EditText
            android:id="@+id/user_upiName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00000000"
            android:gravity="center_vertical"
            android:hint="Your Name"
            android:inputType="text"
            android:paddingLeft="@dimen/dimen_10"
            android:textColor="@color/cpTextDark"
            android:textSize="@dimen/font_size_18"
            app:layout_constraintEnd_toEndOf="@+id/user_upiId"
            app:layout_constraintStart_toStartOf="@+id/user_upiId"
            app:layout_constraintTop_toBottomOf="@+id/user_upiId" />

        <View
            android:id="@+id/use_Vp_title_Uline"
            android:layout_width="0dp"
            android:layout_height="@dimen/dimen_1"
            android:layout_marginTop="@dimen/dimen_15"
            android:background="@color/cpBackgroundbutton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/user_upiName" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:text="Add more payment methods"
            android:textColor="@color/cpPrimeBlue"
            android:textSize="@dimen/font_size_14"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout2"
            app:layout_constraintTop_toBottomOf="@+id/use_Vp_title_Uline" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/payment_info"
            android:gravity="center"
            android:textSize="@dimen/font_size_14"
            android:textColor="@color/cpHelpText"
            android:layout_marginTop="@dimen/dimen_5"
            android:paddingHorizontal="@dimen/dimen_50"
            app:layout_constraintEnd_toEndOf="@+id/textView33"
            app:layout_constraintStart_toStartOf="@+id/textView33"
            app:layout_constraintTop_toBottomOf="@+id/textView33" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>