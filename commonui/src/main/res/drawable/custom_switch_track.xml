<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_30" />
            <solid android:color="@color/cpBackgroundbutton" />
            <stroke android:color="@color/cpBackgroundbutton" android:width="1dp"/>
            <size android:height="11dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_30" />
            <solid android:color="@color/cpTextDark" />
            <stroke android:color="@color/cpTextDark" android:width="1dp"/>
            <size android:height="11dp" />
        </shape>
    </item>
</selector>