                android:lineSpacingExtra="1sp"
                android:maxLines="2"
                android:paddingRight="@dimen/dimen_40"
                android:text="@string/jm_1_know_more"
                android:textColor="@color/cpTextDark"
                android:textSize="@dimen/font_size_16"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView23"
                app:layout_constraintTop_toBottomOf="@+id/textView18" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnNext"
                android:layout_width="@dimen/dimen_0"
                android:layout_height="@dimen/dimen_45"
                android:layout_marginHorizontal="@dimen/dimen_20"
                android:layout_marginBottom="@dimen/dimen_15"
                android:background="@drawable/button_large_background"
                android:elevation="@dimen/dimen_0"
                android:gravity="center"
                android:stateListAnimator="@null"
                android:text="Got It"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/font_size_20"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="@dimen/dimen_3"
                android:text="@string/everyone"
                android:textColor="@color/cpHelpText"
                android:textSize="@dimen/font_size_14"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/head_text" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="@dimen/dimen_0"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_2"
                android:paddingRight="@dimen/dimen_50"
                android:text="@string/jm_anyone_join"
                android:textColor="@color/cpHelpText"
                android:textSize="@dimen/font_size_14"
                app:layout_constraintEnd_toEndOf="@+id/textView19"
                app:layout_constraintStart_toStartOf="@+id/textView19"
                app:layout_constraintTop_toBottomOf="@+id/textView19" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_2"
                android:paddingRight="@dimen/dimen_50"
                android:text="@string/jm_ask_me_anything_sub"
                android:textColor="@color/cpHelpText"
                android:textSize="@dimen/font_size_14"
                app:layout_constraintEnd_toEndOf="@+id/textView23"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView23"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_2"
                android:paddingRight="@dimen/dimen_50"
                android:text="@string/jm_royalty_support"
                android:textColor="@color/cpHelpText"
                android:textSize="@dimen/font_size_14"
                app:layout_constraintEnd_toEndOf="@+id/textView24"
